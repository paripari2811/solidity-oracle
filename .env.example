# CoinMarketCap API Key (get one at https://coinmarketcap.com/api/)
COINMARKETCAP_API_KEY=your-api-key-here

# Private key for the wallet that will send transactions (with 0x prefix)
PRIVATE_KEY=0xyour-private-key-here

# Deployed contract address
CONTRACT_ADDRESS=0xyour-contract-address-here

# RPC URL (defaults to local anvil node)
RPC_URL=http://127.0.0.1:8545
